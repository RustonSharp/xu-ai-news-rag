[run]
source = .
omit = 
    tests/*
    */tests/*
    */test_*
    setup.py
    */site-packages/*
    */venv/*
    */env/*
    */__pycache__/*
    */migrations/*
    */node_modules/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod
    # Have to re-enable the standard pragma
    pragma: no cover

[html]
directory = htmlcov
